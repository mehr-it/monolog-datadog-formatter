{
  "name": "mehr-it/monolog-datadog-formatter",
  "description": "Monolog formatter for datadog JSON logs (timestamp instead of datetime and defaults with stacktrace)",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\MonologDatadogFormatter\\": "src/"
